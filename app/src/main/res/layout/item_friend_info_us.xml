<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginLeft="@dimen/_2sdp"
    android:layout_marginRight="@dimen/_1sdp"
    android:layout_width="@dimen/_90sdp"
    android:background="@drawable/item_bg_reels"
    android:layout_height="@dimen/_130sdp">

    <com.makeramen.roundedimageview.RoundedImageView
        android:id="@+id/imgreels"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_80sdp"
        android:background="@drawable/item_reels_bg_img"
        android:scaleType="centerCrop"
        android:layout_marginTop="@dimen/_10sdp"
        android:src="@drawable/imgbachground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:riv_corner_radius="@dimen/_45sdp"
        tools:ignore="MissingConstraints" />
<!--    <androidx.constraintlayout.widget.ConstraintLayout-->
<!--        android:id="@+id/constraintLayout2"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="match_parent"-->
<!--        android:background="@drawable/item_reels_bg_name">-->

        <TextView
            android:id="@+id/txttextname"
            android:layout_width="0dp"
            android:layout_margin="@dimen/_10sdp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:fontFamily="@font/roboto_bold"
            android:text="122323"
            android:textAlignment="center"
            android:textColor="@color/appcolor"
            android:textSize="@dimen/_10sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="parent" />
